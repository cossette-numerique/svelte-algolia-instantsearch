{
  "name": "@valoche/svelte-algolia-instantsearch",
  "version": "1.0.1",
  "license": "MIT",
  "description": "Svelte wrapper for Algolia InstantSearch",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymeric-giraudet/svelte-algolia-instantsearch.git"
  },
  "keywords": [
    "svelte",
    "algolia",
    "instantsearch",
    "search",
    "instantsearch.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint --max-warnings=0 .",
    "format": "prettier --write .",
    "release": "shipjs prepare"
  },
  "dependencies": {
    "algoliasearch-helper": "3.14.0",
    "instantsearch.js": "4.56.9",
    "qs": "^6.13.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "3.0.3",
    "@sveltejs/kit": "1.24.0",
    "@sveltejs/package": "2.2.2",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "algoliasearch": "4.19.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "shipjs": "0.26.3",
    "svelte": "4.2.0",
    "svelte-check": "3.5.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vitest": "0.34.3"
  },
  "peerDependencies": {
    "@sveltejs/kit": ">= 1 < 3",
    "algoliasearch": ">= 3.1 < 5",
    "svelte": ">= 3 < 5"
  },
  "type": "module",
  "bugs": {
    "url": "https://github.com/aymeric-giraudet/svelte-algolia-instantsearch/issues"
  },
  "homepage": "https://github.com/aymeric-giraudet/svelte-algolia-instantsearch#readme",
  "author": "valoche"
}
